<%- include("partials/header") -%>
    <h1>COMPOSE</h1>
    <form action="/compose" method="post">
        <div class="form-group">
            <label for="title">Title</label>
            <input autocomplete="off" class="form-control" type="text" name="postTitle" placeholder="">
            <label for="para">Post</label>
            <textarea autocomplete="off" class="form-control" name="postBody" id="para" cols="30" rows="10"></textarea>
        </div>
        <button class="btn btn-primary" name="" type="submit">Publish</button>
    </form>
    <%- include("partials/footer") -%>